{
    "name": "goaop/goaop-laravel-bridge",
    "description": "Integration bridge for Go! AOP framework",
    "type": "library",
    "keywords": ["bridge", "laravel", "lumen", "aop", "php", "aspect"],
    "require": {
        "php": ">=5.5.0",
        "goaop/framework": "^1.0|^2.0",
        "illuminate/support": "^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lisachenko Alexander",
            "email": "lisachenko.it@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Go\\Laravel\\GoAopBridge\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Go\\Laravel\\GoAopBridge\\Tests\\": "./tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Go\\Laravel\\GoAopBridge\\GoAopServiceProvider"
            ]
        }
    }
}
