{
    "name": "walterwoshid/goaop-laravel-bridge",
    "description": "Integration bridge for Go! AOP framework",
    "type": "library",
    "keywords": ["bridge", "laravel", "aop", "php", "aspect"],
    "require": {
        "walterwoshid/framework": "^1.0.0",
        "illuminate/support": "^8"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "walterwoshid/goaop-framework",
                "version": "^1.0.0",
                "type": "package",
                "source": {
                    "url": "git@github.com:/walterwoshid/goaop-framework.git",
                    "type": "git",
                    "reference": "fork"
                }
            }
        },
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lisachenko Alexander",
            "email": "lisachenko.it@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Go\\Laravel\\GoAopBridge\\": "./src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Go\\Laravel\\GoAopBridge\\GoAopServiceProvider"
            ]
        }
    }
}
